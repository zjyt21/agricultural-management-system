ction reset() {\n      this.crop = \"\", this.loadPage();\n    },\n    cancel: function cancel() {\n      this.$message.warning(\"Operation successfully canceled\");\n    },\n    formCancel: function formCancel() {\n      this.dialogFormVisible = false;\n      this.$message.warning(\"Form submission canceled\");\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiNA;AACA;EACAA,gBADA;EAEAC,cAFA;EAKAC,IALA,kBAKA;IACA;MACAC,aADA;MAEAC,QAFA;MAGAC,cAHA;MAIAC,WAJA;MAKAC,QALA;MAMAC,QANA;MAOAC,qBAPA;MAQAC,wBARA;MAUAC,kCAVA;MAWAC,iBAXA;MAYAC;IAZA;EAcA,CApBA;EAqBAC,OArBA,qBAqBA;IACA;EACA,CAvBA;EAwBAC;IACAC,QADA,sBACA;MACA;;MACA;QACA;QACA;MACA,CAHA,MAGA;QACA;QACA;MACA;IACA,CAVA;IAWAC,QAXA,sBAWA;MACA;IACA,CAbA;IAcAC,QAdA,sBAcA;MAAA;;MACAC;QACAC;UACAf,6BADA;UAEAC,uBAFA;UAGAC;QAHA;MADA,GAMAc,IANA,CAMA;QACAC;QACA;QACA;MACA,CAVA;IAWA,CA1BA;IA2BAC,gBA3BA,4BA2BAjB,QA3BA,EA2BA;MACA;MACA;IACA,CA9BA;IA+BAkB,mBA/BA,+BA+BAnB,WA/BA,EA+BA;MACA;MACA;IACA,CAlCA;IAmCAoB,SAnCA,uBAmCA;MACA;MACA;IACA,CAtCA;IAuCAC,IAvCA,kBAuCA;MAAA;;MACAP;QACA;UACA;;UACA;;UACA;QACA,CAJA,MAIA;UACA;QACA;MACA,CARA;IASA,CAjDA;IAkDAQ,UAlDA,sBAkDAC,GAlDA,EAkDA;MACA;MACA;IACA,CArDA;IAsDAC,GAtDA,eAsDAC,EAtDA,EAsDA;MAAA;;MACAX;QACA;UACA;;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CA/DA;IAgEAY,qBAhEA,iCAgEAC,GAhEA,EAgEA;MACA;IACA,CAlEA;IAmEAC,QAnEA,sBAmEA;MAAA;;MACA;QAAA;MAAA;MACAd;QACA;UACA;;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CA7EA;IA8EAe,KA9EA,mBA8EA;MACA,gBACA,eADA;IAEA,CAjFA;IAkFAC,MAlFA,oBAkFA;MACA;IACA,CApFA;IAqFAC,UArFA,wBAqFA;MACA;MACA;IACA;EAxFA;AAxBA","names":["name","components","data","tableData","total","currentPage","pageSize","crop","form","multipleSelection","dialogFormVisible","collapseBtnClass","isCollapse","sideWidth","created","methods","collapse","rowClass","loadPage","request","params","then","console","handleSizeChange","handleCurrentChange","handleAdd","save","handleEdit","row","del","id","handleSelectionChange","val","delBatch","reset","cancel","formCancel"],"sourceRoot":"src/views","sources":["HomeView.vue"],"sourcesContent":["<template>\n  <el-container style=\"min-height: 100vh\">\n    <el-aside :width=\"sideWidth + 'px'\" style=\"background-color: rgb(48, 65, 86); box-shadow: 2px 0 6px rgb(0 21 21 / 35%);\">\n      <el-menu :default-openeds=\"['1']\" style=\"min-height: 100%; overflow-x:hidden\" \n                background-color=\"rgb(48, 65, 86)\"\n                text-color=\"#fff\"\n                active-text-color=\"#ffd04b\"\n                :collapse-transition=\"false\"\n                :collapse=\"isCollapse\">\n        <div style=\"height: 60px; text-align: center\">\n          <img src=\"../assets/logo.png\"  height=\"100%\" alt=\"\">\n        </div>\n        <div style=\"height: 20px; text-align: center; color:chartreuse;\" v-show=\"!isCollapse\">\n          <b>Farmer Centre</b>\n        </div>\n        <el-menu-item index=\"0\">\n          <i class=\"el-icon-school\"></i>\n          <template slot=\"title\">\n            <span>Home</span>\n          </template>\n        </el-menu-item>\n        <el-submenu index=\"1\">\n          <template slot=\"title\">\n            <i class=\"el-icon-coin\"></i>\n            <span>Data Records</span>\n          </template>\n          <el-menu-item index=\"1-1\">Agricultural Behavior</el-menu-item>\n          <el-menu-item index=\"1-2\">Soil and Weather</el-menu-item>\n          <el-menu-item index=\"1-3\">History Harvest</el-menu-item>\n        </el-submenu>\n        <el-menu-item index=\"2\">\n          <i class=\"el-icon-s-marketing\"></i>\n          <template slot=\"title\">\n            <span>Market Details</span>\n          </template>\n        </el-menu-item>\n        <el-submenu index=\"3\">\n          <template slot=\"title\">\n            <i class=\"el-icon-s-data\"></i>\n            <span>Data Analysis</span>\n          </template>\n          <el-menu-item index=\"3-1\">Soil Assessment Model</el-menu-item>\n          <el-menu-item index=\"3-2\">Yield Estimation Model</el-menu-item>\n        </el-submenu>\n        <el-submenu index=\"4\">\n          <template slot=\"title\">\n            <i class=\"el-icon-s-custom\"></i>\n            <span>Staff</span>\n          </template>\n          <el-menu-item index=\"4-1\">Authority Management</el-menu-item>\n          <el-menu-item index=\"4-2\">Operation Record</el-menu-item>\n        </el-submenu>\n      </el-menu>\n    </el-aside>\n    \n    <el-container>\n      <el-header style=\"font-size: 12px; border-bottom: 1px solid #ccc; box-shadow: 0px 1px 3px #ccc; line-height: 60px; display: flex\">\n        <div style=\"flex: 1; font-size: 18px\">\n          <span :class=\"collapseBtnClass\" style=\"cursor: pointer\" @click=\"collapse\"></span>\n        </div>\n        <el-dropdown style=\"width: 70px; cursor: pointer\">\n          <span>BOSS</span><i class=\"el-icon-arrow-down\" style=\"margin-left: 5px\"></i>\n          <el-dropdown-menu slot=\"dropdown\">\n\n            <el-dropdown-item class=\"el-icon-user-solid\">Your Farm</el-dropdown-item>\n            <br>\n            <el-dropdown-item class=\"el-icon-right\">Sign Out</el-dropdown-item>\n          </el-dropdown-menu>\n        </el-dropdown>\n      \n      </el-header>\n      \n      <el-main>\n        <div style=\"margin-bottom: 30px;\">\n            <el-breadcrumb separator=\"/\">\n              <el-breadcrumb-item :to=\"{ path: '/' }\">Home</el-breadcrumb-item>\n              <el-breadcrumb-item>Market Information</el-breadcrumb-item>\n            </el-breadcrumb>\n          </div>\n\n        <!-- <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\n          <el-form-item label=\"审批人\">\n            <el-input v-model=\"formInline.user\" placeholder=\"审批人\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"活动区域\">\n            <el-select v-model=\"formInline.region\" placeholder=\"活动区域\">\n              <el-option label=\"区域一\" value=\"shanghai\"></el-option>\n              <el-option label=\"区域二\" value=\"beijing\"></el-option>\n            </el-select>\n          </el-form-item>\n          <el-form-item>\n            <el-button type=\"primary\" @click=\"onSubmit\">查询</el-button>\n          </el-form-item>\n        </el-form> -->\n        <div style=\"padding: 10px 0;\">\n          <el-input style=\"width: 200px;\" placeholder=\"please input the crop name\" suffix-icon=\"el-icon-search\" v-model=\"crop\"></el-input>\n          <el-input style=\"width: 200px;\" placeholder=\"select the begin date\" suffix-icon=\"el-icon-date\"></el-input>\n          <el-input style=\"width: 200px;\" placeholder=\"select the end date\" suffix-icon=\"el-icon-date\"></el-input>\n          <el-button class=\"ml-5\" type=\"primary\" @click=\"loadPage\">Search</el-button>\n          <el-button class=\"ml-5\" type=\"warning\" @click=\"reset\">Reset</el-button>\n        </div>\n\n        <div style=\"margin: 10px 0;\">\n          <el-button type=\"primary\" @click=\"handleAdd\">Add <i class=\"el-icon-circle-plus-outline\"></i></el-button>\n          <el-popconfirm\n            class=\"ml-5\"\n            confirm-button-text='DELETE'\n            cancel-button-text='CANCEL'\n            icon=\"el-icon-info\"\n            icon-color=\"red\"\n            title=\"Are you sure you want to delete these data?\"\n            @confirm=\"delBatch\"\n            @cancel=\"cancel\"\n          >\n          <el-button type=\"danger\" slot=\"reference\">Delete in bulk <i class=\"el-icon-remove-outline\"></i></el-button>\n          </el-popconfirm>\n          <el-button type=\"primary\" class=\"ml-5\">Import <i class=\"el-icon-document-add\"></i></el-button>\n          <el-button type=\"primary\" class=\"ml-5\">Download <i class=\"el-icon-download\"></i></el-button>\n        </div>\n\n        <el-table \n          :data=\"tableData\" \n          :default-sort = \"{prop: 'date', order: 'descending'}\" \n          border \n          stripe \n          :header-cell-style=\"{ textAlign: 'center', background:'beige' }\" \n          :cell-style=\"rowClass\"\n          @selection-change=\"handleSelectionChange\">\n          \n          <div slot=\"empty\" style=\"text-align: left;\">\n            <el-empty description=\"Sorry, there is no data you are looking for\" />\n          </div>\n          <el-table-column type=\"selection\" width=\"55\"></el-table-column>\n          <el-table-column prop=\"date\" label=\"Date\" sortable width=\"100\">\n          </el-table-column>\n          <el-table-column prop=\"crop\" label=\"Crop\" width=\"150\">\n          </el-table-column>\n          <el-table-column prop=\"unitPrice\" label=\"Unit Price (GBP per ton)\" sortable>\n          </el-table-column>\n          <el-table-column prop=\"supply\" label=\"Supply (10,000 tons)\" >\n          </el-table-column>\n          <el-table-column prop=\"demand\" label=\"Demand (10,000 tons)\" >\n          </el-table-column>\n          <el-table-column prop=\"option\" label=\"Operation\" width=\"200\">\n            <template slot-scope=\"scope\">\n              <el-button type=\"success\" @click=\"handleEdit(scope.row)\">Edit <i class=\"el-icon-edit\"></i></el-button>\n              <el-popconfirm\n                class=\"ml-5\"\n                confirm-button-text='DELETE'\n                cancel-button-text='CANCEL'\n                icon=\"el-icon-info\"\n                icon-color=\"red\"\n                title=\"Are you sure to delete?\"\n                @confirm=\"del(scope.row.id)\"\n                @cancel=\"cancel\"\n              >\n              <el-button type=\"danger\" slot=\"reference\">Delete <i class=\"el-icon-remove-outline\"></i></el-button>\n              </el-popconfirm>\n            </template>\n          </el-table-column>\n        </el-table>\n        <div style=\"padding: 10px 0;\">\n          <el-pagination\n            @size-change=\"handleSizeChange\"\n            @current-change=\"handleCurrentChange\"\n            :current-page=\"currentPage\"\n            :page-sizes=\"[5, 10, 20]\"\n            :page-size=\"5\"\n            layout=\"total, sizes, prev, pager, next, jumper\"\n            :total=\"total\">\n          </el-pagination>\n        </div>\n\n        <el-dialog title=\"Market Information\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\n          <el-form label-width=\"auto\" size=\"small\">\n            <el-form-item label=\"Date\">\n              <el-input v-model=\"form.date\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"Crop\" prop=\"crop\">\n              <el-select v-model=\"form.crop\" placeholder=\"Please select a crop name\">\n                <el-option label=\"paddy\" value=\"paddy\"></el-option>\n                <el-option label=\"potato\" value=\"potato\"></el-option>\n                <el-option label=\"soybean\" value=\"soybean\"></el-option>\n                <el-option label=\"peanut\" value=\"peanut\"></el-option>\n                <el-option label=\"wheat\" value=\"wheat\"></el-option>\n                <el-option label=\"barley\" value=\"barley\"></el-option>\n              </el-select>\n            </el-form-item>\n            <el-form-item label=\"Unit Price\">\n              <el-input v-model=\"form.unitPrice\" autocomplete=\"off\" placeholder=\"(GBP per ton)\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"Supply\">\n              <el-input v-model=\"form.supply\" autocomplete=\"off\" placeholder=\"(in 10,000 tons)\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"Demand\">\n              <el-input v-model=\"form.demand\" autocomplete=\"off\" placeholder=\"(in 10,000 tons)\"></el-input>\n            </el-form-item>\n          </el-form>\n          <div slot=\"footer\" class=\"dialog-footer\">\n            <el-button @click=\"formCancel\">Cancel</el-button>\n            <el-button type=\"primary\" @click=\"save\">Confirm</el-button>\n          </div>\n        </el-dialog>\n      </el-main>\n    </el-container>\n  </el-container>\n</template>\n\n<script>\nimport request from '@/util/request'\n  export default {\n    name: 'HomeView',\n    components: {\n      \n    },\n    data() {\n      return {\n        tableData: [],\n        total:0,\n        currentPage:1,\n        pageSize:5,\n        crop:\"\",\n        form:{},\n        multipleSelection: [],\n        dialogFormVisible: false,\n\n        collapseBtnClass:'el-icon-s-fold',\n        isCollapse: false,\n        sideWidth: 250\n      }\n    },\n    created() {\n      this.loadPage()\n    },\n    methods:{\n      collapse(){\n        this.isCollapse = !this.isCollapse\n        if(this.isCollapse){\n          this.sideWidth = 64\n          this.collapseBtnClass = 'el-icon-s-unfold'\n        }else{\n          this.sideWidth = 250\n          this.collapseBtnClass = 'el-icon-s-fold'\n        }\n      },\n      rowClass(){\n        return \"text-align:center\"\n      },\n      loadPage(){\n        request.get(\"/market-trend/page\", {\n          params:{\n            currentPage: this.currentPage,\n            pageSize: this.pageSize,\n            crop:this.crop\n          }\n        }).then(res => {\n            console.log(res)\n            this.tableData = res.records\n            this.total = res.total\n          })\n      },\n      handleSizeChange(pageSize){\n        this.pageSize = pageSize\n        this.loadPage()\n      },\n      handleCurrentChange(currentPage){\n        this.currentPage = currentPage\n        this.loadPage()\n      },\n      handleAdd(){\n        this.dialogFormVisible = true\n        this.form = {}\n      },\n      save(){\n        request.post(\"/market-trend\", this.form).then(res => {\n          if(res){\n            this.$message.success(\"Successfully saved\")\n            this.dialogFormVisible = false;\n            this.loadPage()\n          }else{\n             this.$message.error(\"Failed to save, please try again\")\n          }\n        })\n      },\n      handleEdit(row){\n        this.form = row\n        this.dialogFormVisible = true\n      },\n      del(id){\n        request.delete(\"/market-trend/\" + id).then(res => {\n          if(res){\n            this.$message.success(\"Successfully deleted\")\n            this.loadPage()\n          }else{\n            this.$message.error(\"Failed to delete\")\n          }\n        })\n      },\n      handleSelectionChange(val){\n        this.multipleSelection = val\n      },\n      delBatch(){\n        let ids = this.multipleSelection.map(v => v.id)\n        request.post(\"/market-trend/batchDel\", ids).then(res => {\n          if(res){\n            this.$message.success(\"Successfully deleted\")\n            this.loadPage()\n          }else{\n            this.$message.error(\"Failed to delete\")\n          }\n        })\n      },\n      reset(){\n        this.crop = \"\",\n        this.loadPage()\n      },\n      cancel(){\n        this.$message.warning(\"Operation successfully canceled\")\n      },\n      formCancel(){\n        this.dialogFormVisible = false\n        this.$message.warning(\"Form submission canceled\")\n      },\n    }\n  }\n</script>\n"]},"metadata":{},"sourceType":"module"}