ass = 'el-icon-s-unfold';\n      } else {\n        this.sideWidth = 250;\n        this.collapseBtnClass = 'el-icon-s-fold';\n      }\n    },\n    rowClass: function rowClass() {\n      return \"text-align:center\";\n    },\n    loadPage: function loadPage() {\n      var _this = this;\n\n      request.get(\"/market-trend/page\", {\n        params: {\n          currentPage: this.currentPage,\n          pageSize: this.pageSize,\n          crop: this.crop\n        }\n      }).then(function (res) {\n        console.log(res);\n        _this.tableData = res.records;\n        _this.total = res.total;\n      });\n    },\n    handleSizeChange: function handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.loadPage();\n    },\n    handleCurrentChange: function handleCurrentChange(currentPage) {\n      this.currentPage = currentPage;\n      this.loadPage();\n    },\n    handleAdd: function handleAdd() {\n      var _this2 = this;\n\n      console.log(this.$refs.form);\n      this.dialogFormVisible = true;\n      this.$nextTick(function () {\n        _this2.$refs.form.resetFields();\n      });\n      this.form = {};\n    },\n    save: function save() {\n      var _this3 = this;\n\n      console.log(this.form);\n      request.post(\"/market-trend\", this.form).then(function (res) {\n        if (res) {\n          _this3.$message.success(\"Successfully saved\");\n\n          _this3.dialogFormVisible = false;\n\n          _this3.loadPage();\n        } else {\n          _this3.$message.error(\"Failed to save, please try again\");\n        }\n      });\n    },\n    handleEdit: function handleEdit(row) {\n      this.form = row;\n      this.dialogFormVisible = true;\n    },\n    del: function del(id) {\n      var _this4 = this;\n\n      request.delete(\"/market-trend/\" + id).then(function (res) {\n        if (res) {\n          _this4.$message.success(\"Successfully deleted\");\n\n          _this4.loadPage();\n        } else {\n          _this4.$message.error(\"Failed